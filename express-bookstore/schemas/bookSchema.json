{
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Add Book Schema",
    "required": [
        "isbn",
        "amazon_url",
        "author",
        "language",
        "pages",
        "publisher",
        "title",
        "year"
    ],
    "properties": {
        "isbn": {
            "type": "string",
            "default": "",
            "title": "The isbn Schema",
            "examples": [
                "test"
            ]
        },
        "amazon_url": {
            "type": "string",
            "default": "",
            "title": "The amazon_url Schema",
            "examples": [
                "http://test.com"
            ]
        },
        "author": {
            "type": "string",
            "default": "",
            "title": "The author Schema",
            "examples": [
                "author name"
            ]
        },
        "language": {
            "type": "string",
            "default": "",
            "title": "The language Schema",
            "examples": [
                "english"
            ]
        },
        "pages": {
            "type": "integer",
            "default": 0,
            "title": "The pages Schema",
            "examples": [
                666
            ]
        },
        "publisher": {
            "type": "string",
            "default": "",
            "title": "The publisher Schema",
            "examples": [
                "Book publisher"
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Book Title"
            ]
        },
        "year": {
            "type": "integer",
            "default": 0,
            "title": "The year Schema",
            "examples": [
                1998
            ]
        }
    },
    "examples": [{
        "isbn": "test",
        "amazon_url": "http://test.com",
        "author": "author name",
        "language": "english",
        "pages": 666,
        "publisher": "Book publisher",
        "title": "Book Title",
        "year": 1998
    }]
}